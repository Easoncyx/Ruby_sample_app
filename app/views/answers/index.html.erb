<% provide(:title, 'All Questions Answers') %>
<% if validator? %>
  <h1>Validate <%=@company_name%>'s answers</h1>
<% else %>
  <h1>Questionnaire</h1>
<% end %>
<%= render 'questionnaire' %>
<h3>Output Questions</h3>
<%= button_to "Output Questions", answers_path(format: :csv), class: 'short-input', method: :get, :form => {"data-type" => "csv" } %>