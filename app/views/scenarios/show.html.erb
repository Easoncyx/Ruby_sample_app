<% provide(:title, 'All Scenarios') %>



<ul class="scenarios">
 
  <%= render @scenario %>

</ul>

<table class="table table-hover">
  <thead>
    <tr>
      
      <th scope="col">Name</th>
      <th scope="col">Company Score</th>
      <th scope="col">Category Score</th>
      <th scope="col">Subcategory Score</th>
      <th scope="col">Weight</th>
    </tr>
  </thead>
  <tbody>
    <% @allcompanies.each do |company| %>
    <tr>
      <td><%= get_company_name(company)%></td>
      <td><%= company.score%></td>
      <td><%= get_caegory_score(company)%></td>
    </tr>
    <% end %>
  </tbody>
</table>